# Terraform Environment Variables
# Copy this file to .env and update values as needed

# Environment mode
ENV=local

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# Terraform Variables (using TF_VAR_ prefix)
TF_VAR_project_name=ecom-spa
TF_VAR_environment=dev
TF_VAR_aws_region=us-west-2

# DynamoDB Configuration
TF_VAR_dynamodb_billing_mode=PAY_PER_REQUEST
TF_VAR_enable_point_in_time_recovery=true
TF_VAR_dynamodb_deletion_protection=false

# Application Configuration
TF_VAR_jwt_secret_key=your-super-secret-jwt-key-change-in-production

# Networking
TF_VAR_vpc_cidr=10.0.0.0/16

# State Management (for remote state)
# TF_VAR_state_bucket=your-terraform-state-bucket
# TF_VAR_state_key=ecom-spa/terraform.tfstate
# TF_VAR_state_region=us-west-2
